<div>
<h2>Edit Your Profile</h2>

<%= render 'layouts/flash' %>

<%= form_for current_user do |f| %>
    <div>
    <%= f.label :full_name %><br>
    <%= f.text_field :full_name %>
    </div><br>
    <div>
    <%= f.label :username %><br>
    <%= f.text_field :username %>
    </div><br>
    <%= f.submit "Update Profile" %>
<% end %>
</div>
