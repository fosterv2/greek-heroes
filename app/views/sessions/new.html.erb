<%= form_for @user, controller: 'sessions', action: 'create' do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
  
    <%= f.label :password %>
    <%= f.password_field :password %>
  
    <%= f.submit "login" %>
    Or <%= link_to(controller: 'users', action: 'new') { 'sign up' } %>
<% end %>
